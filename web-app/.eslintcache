        })\r\n        .catch((error) => {\r\n          console.log(error.toString());\r\n        });\r\n    } catch (error) {\r\n      console.log(\"dans l'erreur\", error);\r\n    }\r\n  };\r\n  return (\r\n    <div>\r\n      <Tooltip title=\"Add\" aria-label=\"add\" onClick={() => handleClickOpen()}>\r\n        <Fab color=\"secondary\" className={classes.buttons}>\r\n          <AddIcon />\r\n        </Fab>\r\n      </Tooltip>\r\n      <Dialog\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"form-dialog-title\"\r\n      >\r\n        <DialogTitle id=\"form-dialog-title\">\r\n          Ajout d'un membre du staff :\r\n        </DialogTitle>\r\n        <DialogContent>\r\n          <DialogContentText>\r\n            Veuillez ajouter les informations du staff.\r\n          </DialogContentText>\r\n          <TextField\r\n            autoFocus\r\n            color=\"secondary\"\r\n            margin=\"dense\"\r\n            id=\"standard-basic\"\r\n            label=\"Prénom\"\r\n            type=\"text\"\r\n            fullWidth\r\n            autoComplete=\"no\"\r\n            required\r\n            onChange={(e) =>\r\n              dispatch({\r\n                type: \"fill_input\",\r\n                fieldName: \"firstname\",\r\n                payload: e.currentTarget.value,\r\n              })\r\n            }\r\n          />\r\n          <TextField\r\n            color=\"secondary\"\r\n            margin=\"dense\"\r\n            id=\"standard-basic\"\r\n            label=\"Nom De Famille\"\r\n            type=\"text\"\r\n            fullWidth\r\n            autoComplete=\"no\"\r\n            required\r\n            onChange={(e) =>\r\n              dispatch({\r\n                type: \"fill_input\",\r\n                fieldName: \"lastname\",\r\n                payload: e.currentTarget.value,\r\n              })\r\n            }\r\n          />\r\n          <TextField\r\n            color=\"secondary\"\r\n            margin=\"den